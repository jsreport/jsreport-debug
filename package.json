{
  "name": "jsreport-debug",
  "version": "1.0.1",
  "description": "jsreport extension collecting and serving logs",
  "scripts": {
    "test": "mocha test --timeout 4000 && standard",
    "build": "npm run prepublish",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-debug",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-debug.git"
  },
  "keywords": [
    "jsreport"
  ],
  "license": "MIT",
  "dependencies": {
    "in-publish": "2.0.0"
  },
  "author": "Jan Blaha",
  "standard": {
    "ignore": [
      "main.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "devDependencies": {
    "babel-eslint": "6.0.4",
    "eslint": "2.11.1",
    "eslint-config-standard-jsx": "1.2.1",
    "eslint-plugin-babel": "3.2.0",
    "jsreport-core": "next",
    "jsreport-studio-dev": "0.2.8",
    "mocha": "2.5.3",
    "should": "9.0.1",
    "standard": "7.1.2"
  }
}

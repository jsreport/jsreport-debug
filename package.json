{
  "name": "jsreport-debug",
  "version": "1.1.0",
  "description": "jsreport extension collecting and serving logs",
  "scripts": {
    "test": "mocha test --timeout 4000 && standard",
    "build": "npm run prepublish",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-debug",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-debug.git"
  },
  "keywords": [
    "jsreport"
  ],
  "license": "MIT",
  "dependencies": {
    "in-publish": "2.0.0",
    "safe-buffer": "5.1.1"
  },
  "author": "Jan Blaha",
  "standard": {
    "ignore": [
      "main.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "devDependencies": {
    "babel-eslint": "8.2.1",
    "eslint": "4.16.0",
    "eslint-config-standard-jsx": "4.0.2",
    "eslint-plugin-babel": "4.1.2",
    "jsreport-core": "1.5.1",
    "jsreport-studio-dev": "1.1.2",
    "mocha": "5.0.0",
    "should": "13.2.1",
    "standard": "10.0.3"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ]
}
